{
  // json5はスキーマが効かない
  // $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  description: 'preset that merges all PRs automatically if they pass checks',
  extends: ['config:recommended'],
  configMigration: true,
  timezone: 'Asia/Tokyo',
  // デフォルトで2になってるので
  prHourlyLimit: 0,
  rangeStrategy: 'bump',
  // メジャー含め全てオートマージ
  automerge: true,
  autoApprove: true,
}
